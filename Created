4)CASE
  
  DECLARE
  GRADE CHAR(1);
  BEGIN
  GRADE:='A';
  CASE GRADE
  WHEN 'O' THEN
  DBME_OUTPUT.PUT_LINE('OUTSTANDING');
  WHEN 'A' THEN
  DBME_OUTPUT.PUT_LINE('EXCELLENT');
  WHEN 'B' THEN
  DBME_OUTPUT.PUT_LINE('GOOD');
  WHEN 'C' THEN
  DBME_OUTPUT.PUT_LINE('SATISFACTORY'); 
  WHEN 'F' THEN
  DBME_OUTPUT.PUT_LINE('FAIL');
  ELSE
  DBMS_OUTPUT.PUT_LINE('INVALID GRADE');
  END CASE;
  END;
  /
